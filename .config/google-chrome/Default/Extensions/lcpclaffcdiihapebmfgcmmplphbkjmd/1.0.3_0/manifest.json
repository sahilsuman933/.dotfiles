{
   "action": {
      "default_icon": "images/icon128.png",
      "default_popup": "popup.html",
      "default_title": "YouTube Feed"
   },
   "author": "Unhook",
   "background": {
      "service_worker": "background.js"
   },
   "content_scripts": [ {
      "all_frames": true,
      "css": [ "css/content.css" ],
      "js": [ "content.js" ],
      "matches": [ "https://*.youtube.com/*", "https://*.youtube-nocookie.com/*" ],
      "run_at": "document_start"
   } ],
   "description": "Block the homepage feed, sidebar suggestions, and end screen feed on YouTube.",
   "icons": {
      "128": "images/icon128.png",
      "16": "images/icon16.png",
      "32": "images/icon32.png",
      "48": "images/icon48.png"
   },
   "key": "",
   "manifest_version": 3,
   "minimum_chrome_version": "88",
   "name": "Block YouTube Feed - Homepage, Sidebar Videos",
   "offline_enabled": true,
   "permissions": [ "storage" ],
   "short_name": "YT Feed",
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "1.0.3"
}
